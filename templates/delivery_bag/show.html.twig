{% extends 'base.html.twig' %}

{% block title %}DeliveryBag{% endblock %}

{% block body %}
    <h1>DeliveryBag</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ delivery_bag.id }}</td>
            </tr>
            <tr>
                <th>DeliveryBag_serviceFee</th>
                <td>{{ delivery_bag.deliveryBagServiceFee }}</td>
            </tr>
            <tr>
                <th>DeliveryBag_buttonAmount</th>
                <td>{{ delivery_bag.deliveryBagButtonAmount }}</td>
            </tr>
            <tr>
